{
    "version": 2,
    "builds": [
        {
            "src": "run.py",
            "use": "@vercel/python@2.0.0",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9",
                "installCommand": "chmod +x build.sh && ./build.sh"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "run.py"
        }
    ],
    "env": {
        "FLASK_APP": "run.py",
        "FLASK_ENV": "production",
        "PYTHONPATH": "."
    }
}
